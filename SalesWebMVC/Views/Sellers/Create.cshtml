@model SalesWebMVC.Data.Entity.SellerEntity
 
@{
    ViewData["Title"] = "Create";
}


<h2>@ViewData["Title"]</h2>

<h4> Seller</h4> 
</hr>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">

            <div class="form-group">
                <label asp-for="DsNome"class="control-label"></label>
                <input asp-for="DsNome" class="form-control" />
            </div>
            </hr>
            <div class="form-group">
                <label asp-for="DsEmail"class="control-label"></label>
                <input asp-for="DsEmail" class="form-control" />
            </div>
            </hr>
            <div class="form-group">
                <label asp-for="DhAniversario" class="control-label"></label>
                <input asp-for="DhAniversario" class="form-control" />
            </div>
            </hr>
            <div class="form-group">
                <label asp-for="NrSalario" class="control-label"></label>
                <input asp-for="NrSalario" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default"/>
            </div>
        </form>
    </div>
</div>
